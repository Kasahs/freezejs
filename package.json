{
  "name": "freezejs",
  "version": "1.0.0",
  "description": "deeply freeze js objects and some related utils",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha --require ts-node/register --require source-map-support/register --recursive src/**/*.spec.ts",
    "report-coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "tsc": "tsc --project ./tsconfig.json"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.spec.ts",
      "**/*.d.ts"  
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kasahs/freezejs.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "Object.freeze"
  ],
  "author": "shasak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Kasahs/freezejs/issues"
  },
  "homepage": "https://github.com/Kasahs/freezejs#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/lodash": "^4.14.74",
    "@types/mocha": "^2.2.43",
    "@types/node": "^8.0.29",
    "chai": "^4.1.2",
    "codecov": "^2.3.0",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.1.6",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.4",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "source-map-support": "^0.4.18",
    "travis-cli": "^1.0.9",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.0"
  },
  "dependencies": {}
}
